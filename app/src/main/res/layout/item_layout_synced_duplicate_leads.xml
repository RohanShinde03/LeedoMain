                                android:gravity="center|start"/>

                            <com.google.android.material.textview.MaterialTextView
                                android:id="@+id/mTv_itemList_Mobile_Number"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_margin="1dp"
                                android:paddingLeft="2dp"
                                android:paddingRight="2dp"
                                android:textColor="@color/BlackLight"
                                android:text="@string/na"
                                android:theme="@style/styleFontMediumText"
                                android:textSize="14dp"
                                android:gravity="center|start"/>

                        </androidx.appcompat.widget.LinearLayoutCompat>

                        <!--Alternative Mobile Number-->
                        <androidx.appcompat.widget.LinearLayoutCompat
                            android:id="@+id/ll_itemList_AlternativeMobile"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <com.google.android.material.textview.MaterialTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_margin="1dp"
                                android:paddingLeft="2dp"
                                android:paddingRight="2dp"
                                android:text="Alternative Mobile Number : "
                                android:theme="@style/styleFontMediumText"
                                android:textColor="@color/main_grey"
                                android:textSize="12dp"
                                android:singleLine="true"
                                android:gravity="center|start"/>

                            <com.google.android.material.textview.MaterialTextView
                                android:id="@+id/mTv_itemList_AlternativeNumber"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_margin="1dp"
                                android:paddingLeft="2dp"
                                android:paddingRight="2dp"
                                android:textColor="@color/BlackLight"
                                android:text="@string/na"
                                android:theme="@style/styleFontMediumText"
                                android:textSize="14dp"
                                android:gravity="center|start"/>

                        </androidx.appcompat.widget.LinearLayoutCompat>

                        <!--Birth Date-->
                        <androidx.appcompat.widget.LinearLayoutCompat
                            android:id="@+id/ll_itemList_DateOfBirth"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <com.google.android.material.textview.MaterialTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_margin="1dp"
                                android:paddingLeft="2dp"
                                android:paddingRight="2dp"
                                android:text="Birth Date : "
                                android:theme="@style/styleFontMediumText"
                                android:textColor="@color/main_grey"
                                android:textSize="12dp"
                                android:singleLine="true"
                                android:gravity="center|start"/>

                            <com.google.android.material.textview.MaterialTextView
                                android:id="@+id/mTv_itemList_DateOfBirth"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_margin="1dp"
                                android:paddingLeft="2dp"
                                android:paddingRight="2dp"
                                android:textColor="@color/BlackLight"
                                android:text="@string/na"
                                android:theme="@style/styleFontMediumText"
                                android:textSize="14dp"
                                android:gravity="center|start"/>

                        </androidx.appcompat.widget.LinearLayoutCompat>

                        <!--Budget -->
                        <androidx.appcompat.widget.LinearLayoutCompat
                            android:id="@+id/ll_itemList_Budget"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <com.google.android.material.textview.MaterialTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_margin="1dp"
                                android:paddingLeft="2dp"
                                android:paddingRight="2dp"
                                android:text="Budget : "
                                android:theme="@style/styleFontMediumText"
                                android:textColor="@color/main_grey"
                                android:textSize="12dp"
